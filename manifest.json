{
  "name": "HTML Cutter",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Cut out HTML fragments from a page.",
  "author": "Reorx <novoreorx@gmail.com>",
  "icons": {
    "16": "logo-48.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+I",
        "mac": "Command+I"
      }
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting"
  ]
}
